{
  "name": "wallot-invest",
  "version": "1.0.0",
  "author": "Guilherme Farias",
  "description": "An investment analysis and evaluation project. wallot-invest uses web scraping techniques to collect statistical data from various websites, enabling automated assessments and the option to create questions for manual analysis.",
  "license": "MIT",
  "homepage": "https://github.com/Guilherme-Farias/wallot-invest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guilherme-Farias/wallot-invest.git"
  },
  "bugs": {
    "url": "https://github.com/Guilherme-Farias/wallot-invest/issues"
  },
  "keywords": [
    "typescript",
    "investment",
    "web",
    "scraping"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "format": "prettier . --write",
    "lint": "eslint . --fix --max-warnings=0",
    "test": "vitest run",
    "test:unit": "vitest run -c ./vitest.unit.config.mts",
    "test:unit:watch": "vitest -c ./vitest.unit.config.mts",
    "test:int": "vitest run -c ./vitest.integration.config.mts",
    "test:int:watch": "vitest -c ./vitest.integration.config.mts",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@types/node": "20.14.11",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "@vitest/coverage-v8": "2.0.4",
    "@vitest/ui": "2.0.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.1",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "tsx": "4.16.2",
    "typescript": "5.5.3",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.4",
    "vitest-mock-extended": "2.0.0"
  }
}
